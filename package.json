{
  "name": "eton-ts",
  "version": "0.1.2",
  "description": "TypeScript implementation of ETON (Extended TOON Object Notation)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "benchmark:reuse": "vite-node benchmarks/benchmark_reuse.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --apply .",
    "format": "biome format --write ."
  },
  "keywords": [
    "eton",
    "toon",
    "serialization",
    "llm",
    "data-format",
    "token-optimization",
    "intelligence-protocol"
  ],
  "author": "kida-hirokazu",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@toon-format/toon": "^2.1.0",
    "@types/iarna__toml": "^2.0.5",
    "@types/node": "^20.11.19",
    "@vitest/coverage-v8": "^4.0.18",
    "gpt-tokenizer": "^3.4.0",
    "typescript": "^5.3.3",
    "vite-node": "^5.3.0",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.4.5"
  }
}